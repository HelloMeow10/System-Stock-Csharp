@page "/"
@namespace AgileStockPro.Web.Pages
@addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelpers
@{
    Layout = null;
}
<!DOCTYPE html>
<html lang="es">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>AgileStockPro</title>
    <base href="/" />
    <link rel="stylesheet" href="/_content/Microsoft.FluentUI.AspNetCore.Components/css/reboot.css" />
    <link rel="stylesheet" href="/css/app.css" />
    <!-- CSS isolation bundle: necesario para estilos de componentes (.razor.css) -->
    <link rel="stylesheet" href="AgileStockPro.Web.styles.css" />
</head>
<body>
    <app>
        <component type="typeof(App)" render-mode="Server" />
    </app>
    <script src="/js/app.js"></script>
    <script src="_framework/blazor.server.js"></script>
</body>
</html>
